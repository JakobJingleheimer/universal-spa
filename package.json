{
  "name": "universal-spa",
  "version": "1.0.0",
  "description": "Quick n dirty universal SPA",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=DEV node_modules/.bin/webpack",
    "build:prod": "NODE_ENV=PROD node_modules/.bin/webpack -p",
    "start": "NODE_ENV=PROD node_modules/.bin/babel-node --presets 'react,es2015' src/server.js"
  },
  "keywords": [
    "universal javascript",
    "universal react"
  ],
  "author": "Jacob Smith <jacob.smith@chimp.net>",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.10",
    "webpack": "^1.14.0"
  }
}
